    "babel-jest": "^23.6.0",
    "raf": "^3.4.0"
  },
  "jest": {
    "testRegex": "\\.spec\\.tsx?$",
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "moduleFileExtensions": [
